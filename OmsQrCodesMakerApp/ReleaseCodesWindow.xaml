<Window
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OmsQrCodesMakerApp"
        xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors" x:Class="OmsQrCodesMakerApp.ReleaseCodesWindow"
        mc:Ignorable="d" ResizeMode="NoResize"
        Title="Выпуск кодов" Height="120" Width="300">
    <Grid>
        <StackPanel Orientation="Vertical">
            <StackPanel Orientation="Horizontal">
                <Label Margin="5 10" Content="Количество кодов" />
                <dxe:SpinEdit x:Name="CodesSpinEdit" Margin="5 10" EditValue="0" Increment="1" MinValue="0" Width="150" />
            </StackPanel>
            <StackPanel Orientation="Horizontal" HorizontalAlignment="Right">
                <Button x:Name="ReleaseButton" Click="ReleaseButton_Click" Margin="5" Content="Выпустить коды" />
                <Button x:Name="CancelButton" Click="CancelButton_Click" Margin="5" Content="Отмена" />
            </StackPanel>
        </StackPanel>
    </Grid>
</Window>
